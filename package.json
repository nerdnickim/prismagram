{
	"name": "prismagram",
	"version": "1.0.0",
	"description": "Instagram clone with Express + Prisma + React and React Native",
	"repository": "https://github.com/nerdnickim/prismagram",
	"author": "Nerdnickk <56470910+Nerdnickk@users.noreply.github.com>",
	"license": "MIT",
	"dependencies": {
		"@babel/cli": "^7.10.1",
		"@babel/core": "^7.9.6",
		"@babel/node": "^7.8.7",
		"@babel/preset-env": "^7.9.6",
		"@babel/runtime": "^7.10.2",
		"aws-sdk": "^2.690.0",
		"copy": "^0.3.2",
		"dotenv": "^8.2.0",
		"graphql-tools": "^5.0.0",
		"graphql-yoga": "^1.18.3",
		"jsonwebtoken": "^8.5.1",
		"merge-graphql-schemas": "^1.7.8",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"multer-s3": "^2.9.0",
		"nodemailer": "^6.4.16",
		"nodemailer-sendgrid": "^1.0.3",
		"nodemailer-sendgrid-transport": "^0.2.0",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"prisma-client-lib": "^1.34.10"
	},
	"scripts": {
		"deploy": "prisma deploy",
		"generate": "prisma generate",
		"prisma": "yarn run deploy && yarn run generate",
		"dev": "nodemon --exec babel-node src/server.js",
		"prebuild": "yarn run prisma",
		"build": "babel src -d build",
		"postbuild": "cd src && copy ./api/**/*.graphql ../build/api/",
		"start": "node build/server.js"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.10.1"
	}
}
